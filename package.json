{
  "name": "snapenv",
  "version": "0.2.0",
  "description": "typesafe env validation with zero dependencies",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "env",
    "environment",
    "variables",
    "validation",
    "typescript",
    "types",
    "dotenv",
    "config",
    "typesafe",
    "schema",
    "parse",
    "process.env"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": "Visible",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/visible/snapenv"
  },
  "homepage": "https://github.com/visible/snapenv",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
